{
  "version": 2,
  "builds": [
    {
      "src": "ui/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "storybook-static"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/storybook-static/$1"
    }
  ],
  "buildCommand": "cd ui && npm run build-storybook",
  "outputDirectory": "storybook-static",
  "installCommand": "cd ui && npm install"
}
